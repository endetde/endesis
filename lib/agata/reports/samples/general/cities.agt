<?xml version="1.0"?>
<Report>
    <Version>7.4</Version>
    <Properties>
        <Description>Cities List</Description>
        <Title>Listagem de Cidades</Title>
        <Author></Author>
        <Keywords></Keywords>
        <Date>2006-01-03</Date>
        <FrameSize></FrameSize>
    </Properties>

    <Header>
        <Body></Body>
        <Align>center</Align>
    </Header>

    <Footer>
