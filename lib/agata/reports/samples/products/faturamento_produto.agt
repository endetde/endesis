    <Parameters>
        <data_inicial>
            <mask>dd/mm/yyyy</mask>
            <value>01/05/2006</value>
        </data_inicial>

        <data_final>
            <mask>dd/mm/yyyy</mask>
            <value>30/06/2006</value>
        </data_final>

    </Parameters>

    <DataSet>
        <DataSource>
            <Name>carvalhaes</Name>
        </DataSource>

        <Query>
            <Select>sf2.f2_hora as "hora", SF2.F2_DOC as "Nota", SF2.F2_EMISSAO as "Emissão", SA1.A1_NOME  as "Cliente", SD2.D2_COD as "Código", SB1.B1_DESC as "Descrição", SD2.D2_QUANT as "Qtd", SD2.D2_PRCVEN as "Unit", SD2.D2_TOTAL as "total", SF2.F2_VEND1, SF2.F2_VEND2, SF2.F2_VEND3, SF2.F2_VEND4</Select>
            <From>SF2010 SF2 INNER JOIN SD2010 SD2 ON (SD2.D2_DOC = SF2.F2_DOC) INNER JOIN SB1010 SB1 ON (SB1.B1_COD = SD2.D2_COD) INNER JOIN SA1010 SA1 ON (SF2.F2_CLIENTE = SA1.A1_COD)</From>
            <Where>F2_EMISSAO &gt;= br2us($data_inicial) and F2_EMISSAO &lt;= br2us($data_final) and SF2.D_E_L_E_T_&lt;&gt; '*' and sd2.d2_tes IN (SELECT f4_codigo FROM sf4010 WHERE d_e_l_e_t_ &lt;&gt; '*' AND f4_duplic = 'S' AND f4_codigo &gt;= '500')</Where>
            <GroupBy></GroupBy>
            <OrderBy></OrderBy>
            <Config>
                <Distinct></Distinct>
                <OffSet>0</OffSet>
                <Limit>0</Limit>
            </Config>

        </Query>

        <Groups>
            <Config>
                <ShowGroup>1</ShowGroup>
                <ShowDetail>1</ShowDetail>
                <ShowLabel></ShowLabel>
                <ShowNumber>1</ShowNumber>
                <ShowIndent>1</ShowIndent>
            </Config>

            <Formulas>
                <Group1>sum(9)</Group1>
                <Group0>sum(9) as 'Total Faturado no período R$ '</Group0>
            </Formulas>

        </Groups>

