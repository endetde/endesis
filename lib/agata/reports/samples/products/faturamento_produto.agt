<?xml version="1.0"?>
<Report>
    <Version>7.4</Version>
    <Properties>
        <Description>Relatório de faturamento agrupado por PRODUTO</Description>
        <Title></Title>
        <Author></Author>
        <Keywords></Keywords>
        <Date></Date>
        <FrameSize></FrameSize>
    </Properties>

    <Header>
        <Body></Body>
        <Align>center</Align>
